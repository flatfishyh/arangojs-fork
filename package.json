    "cucumber": "^0.4.4",
    "dekeywordify": "^0.2.0",
    "dirwatch": "^1.1.0",
    "expect.js": "^0.3",
    "jshint": "^2.5.6",
    "mocha": "^1.21",
    "rewire": "^2.1",
    "uglify-js": "^2.4.15",
    "xhr": "^1.17.0"
